{
  "kind": "collectionType",
  "collectionName": "menus",
  "info": {
    "singularName": "menu",
    "pluralName": "menus",
    "displayName": "Menu"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": false
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "order": {
      "type": "integer"
    },
    "active": {
      "type": "boolean"
    },
    "childItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::menu.menu",
      "mappedBy": "parentItem"
    },
    "linkType": {
      "type": "enumeration",
      "enum": [
        "internal",
        "external"
      ]
    },
    "destination": {
      "type": "string"
    },
    "location": {
      "type": "enumeration",
      "enum": [
        "header",
        "footer",
        "sidebar"
      ]
    },
    "parentItem": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::menu.menu",
      "inversedBy": "childItems"
    }
  }
}
